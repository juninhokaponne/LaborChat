{
  "expo": {
    "name": "imhere2",
    "slug": "imhere2",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "ts",
        "tsx",
        "svg"
      ]
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/Poppins-Regular.ttf"]
        }
      ]
    ]
  }
}
